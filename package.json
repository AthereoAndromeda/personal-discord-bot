{
  "name": "calnatsci-ts",
  "version": "3.1.1",
  "description": "CalNatSci Bot rewritten in TypeScript",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "benchmark": "tsnd benchmark/benchmark",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "node -r dotenv/config .",
    "dev-build": "swc src -d dist",
    "start": "node .",
    "ts-dev": "tsnd --respawn --debug --transpile-only -r dotenv/config -- ./src/index.ts",
    "test": "jest --coverage"
  },
  "engines": {
    "node": ">=16.6.1",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "tsnd prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redstripez08/CalNatSci-TS.git"
  },
  "author": "Angelo <athereoandromeda@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redstripez08/CalNatSci-TS/issues"
  },
  "homepage": "https://github.com/redstripez08/CalNatSci-TS#readme",
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/collection": "^0.3.2",
    "@prisma/client": "^3.4.2",
    "colorette": "^2.0.16",
    "common-tags": "^1.8.1",
    "discord.js": "^13.3.1",
    "got": "^11.8.2",
    "luxon": "^2.1.1",
    "prisma": "^3.4.2",
    "ts-essentials": "^9.0.0",
    "winston": "^3.3.3"
  },
