    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.108",
    "@swc/jest": "^0.2.5",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^27.0.2",
    "@types/luxon": "^2.0.7",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "benny": "^3.7.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "jest-extended": "^1.1.0",
    "jest-mock-extended": "^2.0.4",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.22.8",
